{
  "name": "ronasit/laravel-remote-data-collector",
  "description": "Provides service for data collecting for ronasit/laravel-swagger plugin",
  "keywords": ["laravel", "swagger", "ronasit", "auto-documentation", "rest-api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Dubrovin",
      "email": "rdubrovin@ronasit.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": ">=5.3.0",
    "ronasit/laravel-swagger": ">=1.6.*",
    "ronasit/laravel-helpers": ">=2.0.1-beta"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\DataCollectors\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "RonasIT\\Support\\DataCollectors\\RemoteDataCollectorServiceProvider"
      ]
    }
  }
}
